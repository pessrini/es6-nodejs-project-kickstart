{
  "name": "es6-nodejs-setup",
  "version": "1.0.0",
  "description": "ES6 Node js project starter kit to write Services within a minute using latest tools",
  "main": "server.js",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint --fix . --ext .js",
    "test": "nyc mocha --compilers js:babel-core/register --require babel-polyfill --exit",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "babel src -d dist",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/srinivasKandukuri/es6-nodejs-project-kickstart.git"
  },
  "keywords": [
    "REST services",
    "eslint",
    "javascript",
    "nodejs",
    "boilerplate",
    "es6",
    "es2015",
    "babel",
    "starter kit"
  ],
  "author": "srinivas kandukuri (https://srinivaskandukuri.github.io/profile/)",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "codecov": "^3.1.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  },
  "bugs": {
    "url": "https://github.com/srinivasKandukuri/es6-nodejs-project-kickstart/issues"
  },
  "homepage": "https://github.com/srinivasKandukuri/es6-nodejs-project-kickstart"
}
